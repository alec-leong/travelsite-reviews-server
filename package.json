  "scripts": {
    "lint": "npx eslint .",
    "react-dev": "webpack -d -w",
    "react-prod": "webpack -p",
    "seed": "node database/seed.js",
    "server-dev": "cross-env NODE_ENV=development nodemon server/index.js",
    "server-prod": "nohup cross-env NODE_ENV=production node server/index.js &",
    "ssl": "openssl req -x509 -newkey rsa:2048 -nodes -sha256 -subj //CN=localhost -keyout ./keys/localhost-privkey.pem -out ./keys/localhost-cert.pem",
    "test:react-app": "jest --forceExit tests/App.test.jsx",
    "test:react-search": "node nightwatch.js tests/Search.test.js -e chrome",
    "test:seed": "jest --env=node --forceExit tests/seed.test.js",
    "test:server": "jest --env=node --forceExit tests/server.test.js",
    "test:watch": "npm run test -- --watch",
    "test": "npm run test:seed && npm run test:server && npm run test:react-app && npm run test:react-search",
    "update": "ncu -u && npm install"
  },
